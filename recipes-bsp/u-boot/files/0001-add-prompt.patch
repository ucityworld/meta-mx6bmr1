diff --git a/configs/mx6bmr1_defconfig b/configs/mx6bmr1_defconfig
new file mode 100644
index 0000000..2e7eb4d
--- /dev/null
+++ b/configs/mx6bmr1_defconfig
@@ -0,0 +1,7 @@
+CONFIG_ARM=y
+CONFIG_ARCH_MX6=y
+CONFIG_TARGET_MX6BMR1=y
+CONFIG_SYS_EXTRA_OPTIONS="IMX_CONFIG=board/freescale/mx6bmr1/mx6bmr1.cfg,MX6DL"
+# CONFIG_CMD_IMLS is not set
+# CONFIG_CMD_FLASH is not set
+CONFIG_SPI_FLASH=y
